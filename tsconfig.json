{
  "compilerOptions": {
    "target": "es6",
    "rootDir": "src/app",
    "module": "system",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "doc", "e2e", "NOTES", "src/utils", "src/assets",     "src/app/scenes"] 
}
